/**** Reset style BEGIN ****/

html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
    margin:0;
    padding:0;
    border:0;
    outline:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
}

body {
    line-height:1;
}

article,aside,details,figcaption,figure,
footer,header,hgroup,menu,nav,section {
    display:block;
}

nav ul {
    list-style:none;
}

blockquote, q {
    quotes:none;
}

blockquote:before, blockquote:after,
q:before, q:after {
    content:'';
    content:none;
}

a {
    margin:0;
    padding:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
}

/* change colours to suit your needs */
ins {
    background-color:#ff9;
    color:#000;
    text-decoration:none;
}

/* change colours to suit your needs */
mark {
    background-color:#ff9;
    color:#000;
    font-style:italic;
    font-weight:bold;
}

del {
    text-decoration: line-through;
}

abbr[title], dfn[title] {
    border-bottom:1px dotted;
    cursor:help;
}

table {
    border-collapse:collapse;
    border-spacing:0;
}

/* change border colour to suit your needs */
hr {
    display:block;
    height:1px;
    border:0;
    border-top:1px solid #cccccc;
    margin:1em 0;
    padding:0;
}

input, select {
    vertical-align:middle;
}
/**** Reset style END ****/

:root {
    --deprecated-color: rgb(229, 111, 74);
    --param-label-color: rgb(60, 66, 87);
    --param-constraint-color: rgb(135, 146, 162);
    --body-color: rgb(79, 86, 107);
    --body-bg-color: rgb(79, 86, 107);
    --divider-color: #e3e8ee;
    --nav-heading-color: rgb(163, 172, 185);
    --nav-item-color: rgb(105, 115, 134);
    --nav-item-active-color: rgb(85, 108, 214);
    --nav-item-hover-color: rgb(42, 47, 69);
    --nav-item-selected-color: rgb(85, 108, 214);
    --h4-color: rgb(163, 172, 185);
    --inline-code-color: rgb(205, 61, 100);
    --inline-code-border-color: rgba(0, 0, 0, .05);

    --a-link-color: rgb(85, 108, 214);
    --a-hover-color: rgb(42, 47, 69);

    --code-block-border-radius: 8px;
    --code-block-top-bar-color: rgb(79, 86, 107);
    --code-block-top-bar-bg-color: rgb(236, 236, 236);
    --code-block-source-color: #697386;
    --code-block-source-bg-color: rgb(247, 250, 252);

    --code-block-command-top-bar-color: rgb(193, 201, 210);
    --code-block-command-top-bar-bg-color: rgb(60,66,87);
    --code-block-command-source-color: rgb(245, 251, 255);
    --code-block-command-source-bg-color: rgb(79,86,107);


    --monospace-font: "Menlo", "Consolas", monospace;



}

a:link {
    color: var(--a-link-color);
    text-decoration: none;
}

a:visited {
    color: var(--a-link-color);
}

a:hover {
    color: var(--a-hover-color);
}

p {
    margin-top: 0;
    margin-bottom: 16px;
}

ul {
    list-style: none;
    padding-inline-start: 0;
}


h1 {
    font-size: 24px;
    font-weight: 300;
    letter-spacing: 0.63px;
    line-height: 40px;
    margin-bottom: 16px;
}
h2 {
    font-size: 20px;
    font-weight: 300;
    margin-bottom: 14px;
}
h3 {
    font-size: 14px;
    font-weight: 500;
    text-transform: uppercase;
    margin-bottom: 12px;

}
h4 {
    font-size: 12px;
    font-weight: 500;
    color: var(--h4-color);
    text-transform: uppercase;
    margin-top: 20px;
    padding-bottom: 12px;
    border-bottom: solid 1px var(--divider-color);
}

h5 {
    margin-top: 16px;
    margin-bottom: 0px;
    font-size: 14px;
    font-weight: 400;
}

body {
    font-family: --apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif;
    color: var(--body-color);
    font-size: 14px;
    font-weight: 400;
    line-height: 26px;
    margin: 0px;
}

body, button, html, input {
    -moz-osx-font-smoothing: grayscale;
    -webkit-font-smoothing: antialiased;
}

code {
    font-family: var(--monospace-font);
    color: var(--inline-code-color);
    font-size: 14px;
    font-weight: 500;
    line-height: 26px;
    padding-right: 3px;
    padding-left: 3px;
    padding-top: 0;
    padding-bottom: 0;
    border: 1px solid var(--inline-code-border-color);
    border-radius: 4px;
    max-height: 600px;
}

header {
    margin-bottom: 10px;
}

.hidden {
    display: none;
}

.odd {
    background: #f7fafc;
}

.sidebar {
    border-right: 1px solid var(--divider-color);
}

.logo {
    background-repeat: no-repeat;
    background-size: contain;
    background-origin: content-box;
    background-position: left center;
    width: 100%;
    height: 80px;
}

.nav {
    padding-left: 24px;
    padding-right: 24px;
}
.nav .sidebar-nav-heading {
    text-transform: uppercase;
    padding-top: 8px;
    padding-bottom: 6px;
    font-size: 13px;
    font-weight: 600;
}

.nav .sidebar-nav-heading, .nav .sidebar-nav-heading a:link, .nav .sidebar-nav-heading a:visited {
    color: var(--nav-heading-color);
}

.nav .sidebar-nav-heading a:hover {
    color: var(--nav-heading-hover-color);
}

.nav .sidebar-nav-items {
    line-height: 30px;
    font-size: 14px;
    font-weight: 500;
}

.nav .sidebar-nav-items a:link, .nav .sidebar-nav-items a:visited {
    color: var(--nav-item-color);
}

.nav .sidebar-nav-items a:hover {
    color: var(--nav-item-hover-color);
}

.nav .sidebar-nav-items a.selected {
    color: var(--nav-item-selected-color);
}

.nav .sidebar-nav-item {
    padding-left: 14px;
    padding-right: 4px;
    font-weight: 400;
    line-height: 24px;
}

.grid-container {
    display: grid;
    grid-template-columns: 300px auto;
}

.grid-item {
}

.api-ref-container {
    display: grid;
    grid-template-columns: 55% 45%;
}

.api-ref-item {
    border: 0px solid red;
    min-width: 400px;
}

.api-ref-left {
    padding-right: 100px;
}

.api-ref-right {

}


ul.api-ref-params li {
    margin-top: 16px;
    margin-bottom: 16px;
    min-height: 50px;
}

.api-ref-op-container {
    display: grid;
    grid-template-columns: 12em auto;
    line-height: 1;
}

.api-ref-op-item {
    font-size: 14px;
    padding-right: 8px;
}


.scrollable {
    overflow-y: auto;
    overflow-x: visible;
    max-height: 100vh;
}

.api-container {
    max-width: 1520px;
    margin: auto;
}
.api-container-content {
    padding: 100px;
}


.code-block {
    margin-top: 5px;
    margin-bottom: 10px;
    font-size: 13px;
}

.code-block .top-bar {
    border-radius: 0;
    border-top-left-radius: var(--code-block-border-radius);
    border-top-right-radius: var(--code-block-border-radius);
    padding: 8px;
    font-family: var(--monospace-font);
    font-weight: 500;
    color: var(--code-block-top-bar-color);
    background-color: var(--code-block-top-bar-bg-color);
    display: block;
    text-transform: uppercase;
}

.code-block .source {
    border-radius: 0;
    border-bottom-left-radius: var(--code-block-border-radius);
    border-bottom-right-radius: var(--code-block-border-radius);
    padding: 8px;
    display: block;
    overflow: auto;
    white-space: nowrap;
    padding-bottom: 16px;
    color: var(--code-block-source-color);
    background-color: var(--code-block-source-bg-color);
    white-space: pre;
}

.code-block.command .top-bar {
    color: var(--code-block-command-top-bar-color);
    background-color: var(--code-block-command-top-bar-bg-color);
    text-transform: initial;
}

.code-block.command .source {
    color: var(--code-block-command-source-color);
    background-color: var(--code-block-command-source-bg-color);
}

.op-attr {
    color: var(--param-constraint-color);
    font-size: 12px;
    line-height: 17px;
}
.op-label {
    font-size: 13px;
    font-weight: 600;
    line-height: 17px;
    color: var(--param-label-color);
    word-break: break-word;
}

.op-label > span {
    font-weight: 300;
}

.divider, .horizontal-divider {
    border-bottom: 1px solid var(--divider-color);
}

.horizontal-divider {
    margin-top: 100px;
    margin-bottom: 100px;
}


.deprecated, .required {
    color: var(--deprecated-color);
    font-size: 10px;
    font-weight: 600;
    text-transform: uppercase;
}

.monospace {
    font-family: var(--monospace-font);
}
